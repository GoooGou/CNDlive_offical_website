{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      // mdx,
      "@/components/mdx/*": ["src/components/mdx/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
